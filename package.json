{
  "name": "ngx-formly-designer-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "debug": "concurrently \"ng serve -c development\"  \"ng build ngx-formly-designer --watch\"",
    "start": "npm run build_lib && ng serve --prod",
    "build": "ng build ngx-formly-designer && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy": "node tools/copy.js README.md dist/ngx-formly-designer/README.md && node tools/copy.js LICENSE dist/ngx-formly-designer/LICENSE",
    "build_lib": "ng build ngx-formly-designer --prod",
    "npm_pack": "npm run copy && cd dist/ngx-formly-designer && npm pack",
    "package": "npm run build_lib && npm run npm_pack"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^8.2.5",
    "@angular/core": "^8.2.5",
    "@angular/compiler": "^8.2.5",
    "@angular/animations": "^8.2.5",
    "@angular/forms": "^8.2.5",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.2.5",
    "@angular/platform-browser-dynamic": "^8.2.5",
    "@angular/router": "^8.2.5",
    "@ngx-formly/core": "^5.4.1",
    "@ngx-formly/bootstrap": "^5.4.1",
    "ngx-formly-tabs": "^1.1.0",
    "bootstrap": "^4.3.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "lodash-es": "^4.17.15",
    "popper.js": "^1.15.0",
    "core-js": "^3.2.1",
    "rxjs": "^6.5.3",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^8.2.5",
    "@angular-devkit/build-ng-packagr": "~0.803.3",
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular/cli": "~8.3.3",
    "@angular/language-service": "^8.2.5",
    "@types/jquery": "~3.3.31",
    "@types/lodash-es": "~4.17.3",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.7.4",
    "codelyzer": "~5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.2",
    "ng-packagr": "^5.5.0",
    "tsickle": "0.37.0",
    "tslib": "^1.10.0",
    "typescript": "3.5.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.19.0"
  }
}
