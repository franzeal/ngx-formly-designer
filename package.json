{
  "name": "ngx-formly-designer-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "debug": "concurrently \"ng serve -c development\"  \"ng build ngx-formly-designer --watch\"",
    "start": "npm run build_lib && ng serve --prod",
    "build": "ng build ngx-formly-designer && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy": "node tools/copy.js README.md dist/ngx-formly-designer/README.md",
    "build_lib": "ng build ngx-formly-designer --prod",
    "npm_pack": "npm run copy && cd dist/ngx-formly-designer && npm pack",
    "package": "npm run build_lib && npm run npm_pack"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^7.2.4",
    "@angular/core": "^7.2.4",
    "@angular/compiler": "^7.2.4",
    "@angular/animations": "^7.2.4",
    "@angular/forms": "^7.2.4",
    "@angular/http": "^7.2.4",
    "@angular/platform-browser": "^7.2.4",
    "@angular/platform-browser-dynamic": "^7.2.4",
    "@angular/router": "^7.2.4",
    "@ngx-formly/core": "^4.8.2",
    "@ngx-formly/bootstrap": "^4.8.2",
    "ngx-formly-tabs": "^1.1.0",
    "bootstrap": "^4.1.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "lodash-es": "^4.17.11",
    "popper.js": "^1.14.7",
    "core-js": "^2.6.4",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^7.2.4",
    "@angular-devkit/build-ng-packagr": "~0.13.1",
    "@angular-devkit/build-angular": "~0.13.1",
    "@angular/cli": "~7.3.1",
    "@angular/language-service": "^7.2.4",
    "@types/jquery": "~3.3.29",
    "@types/lodash-es": "~4.17.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.5.6",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.2.0",
    "protractor": "~5.4.0",
    "ng-packagr": "^4.7.0",
    "tsickle": ">=0.34.3",
    "tslib": "^1.9.3",
    "typescript": ">=3.1.1 <3.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0"
  }
}
